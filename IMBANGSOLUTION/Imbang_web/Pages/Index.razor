        text-align: center;
    }

    h1 {
        font-size: 2.5em;
        margin: 0 0 20px 0;
    }

    .destination {
        margin: 20px 0;
        padding: 20px;
        background: rgba(0, 0, 0, 0.5); /* Semi-transparent background for contrast */
        border-radius: 10px;
    }

